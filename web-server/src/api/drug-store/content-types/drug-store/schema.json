{
  "kind": "collectionType",
  "collectionName": "drug_stores",
  "info": {
    "singularName": "drug-store",
    "pluralName": "drug-stores",
    "displayName": "drug_store"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name_th": {
      "type": "string",
      "required": true
    },
    "name_en": {
      "type": "string",
      "required": true
    },
    "license_number": {
      "type": "string",
      "required": true
    },
    "license_doc": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "text",
      "required": true
    },
    "phone_store": {
      "type": "string",
      "required": true
    },
    "time_open": {
      "type": "time",
      "required": true
    },
    "time_close": {
      "type": "time",
      "required": true
    },
    "link_gps": {
      "type": "string",
      "required": true
    },
    "photo_front": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "photo_in": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "photo_staff": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "admin_profile": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::admin-profile.admin-profile",
      "inversedBy": "drug_stores"
    },
    "staff_profiles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::staff-profile.staff-profile",
      "mappedBy": "drug_store"
    },
    "services": {
      "type": "json"
    },
    "type": {
      "type": "enumeration",
      "default": "type1",
      "enum": [
        "type1",
        "type2",
        "type3"
      ]
    },
    "customer_profiles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::customer-profile.customer-profile",
      "mappedBy": "drug_stores"
    },
    "pharmacy_profiles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::pharmacy-profile.pharmacy-profile",
      "inversedBy": "drug_stores"
    }
  }
}
