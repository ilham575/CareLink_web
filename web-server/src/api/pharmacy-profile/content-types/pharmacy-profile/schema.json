{
  "kind": "collectionType",
  "collectionName": "pharmacy_profiles",
  "info": {
    "singularName": "pharmacy-profile",
    "pluralName": "pharmacy-profiles",
    "displayName": "pharmacy_profile"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "license_number": {
      "type": "string"
    },
    "services": {
      "type": "json"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "pharmacy_profiles"
    },
    "drug_stores": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::drug-store.drug-store",
      "mappedBy": "pharmacy_profiles"
    },
    "profileimage": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "working_time": {
      "type": "json"
    },
    "notifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "pharmacy_profile"
    }
  }
}
