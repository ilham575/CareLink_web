{
  "kind": "collectionType",
  "collectionName": "staff_profiles",
  "info": {
    "singularName": "staff-profile",
    "pluralName": "staff-profiles",
    "displayName": "staff_profile"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "staff_profiles"
    },
    "position": {
      "type": "string",
      "required": true
    },
    "profileimage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "time_start": {
      "type": "time"
    },
    "time_end": {
      "type": "time"
    },
    "drug_store": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::drug-store.drug-store",
      "inversedBy": "staff_profiles"
    },
    "working_days": {
      "type": "json",
      "required": false
    },
    "doc_uid": {
      "type": "string",
      "unique": true
    },
    "work_schedule": {
      "type": "json"
    }
  }
}
